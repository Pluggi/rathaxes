ADVICE common_interface PART_OF kernel_interfaces
{
	<%
		foreach interface in this.kernel_interfaces
    	{
            foreach fct in interface.block.functions
            {
			    		%>JOINPOINT @fct.key()@ IN kernel_interfaces.common_interface.@fct.key()@;@"\n"@<%
            }
    	}
	%>
};

