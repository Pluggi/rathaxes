local   sDriverName = this.name;
%>

ADVICE unload PART_OF RKM
{
  /*
  **  LKM exit point
  */
  static void /*__exit*/    @sDriverName@_exit(void)
  {
    JOINPOINT unregistration IN RKM.kernel_registration.unregistration;
    return;
  }
};
