declare function checkInterfaceId(aInterfaceHash : node, sInterfaceName : value);

function checkInterfaceId(aInterfaceHash : node, sInterfaceName : value)
{
	if (findElement(sInterfaceName, aInterfaceHash))
		rtxError("KERNEL_INTERFACES: " + sInterfaceName + " already exists.");
}
