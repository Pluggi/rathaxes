declare function checkSequenceId(aSequenceHash : node, sSequenceName : value);

function checkSequenceId(aSequenceHash : node, sSequenceName : value)
{
	if (findElement(sSequenceName, aSequenceHash))
		rtxError("SEQUENCES: " + sSequenceName + " already exists.");
}
